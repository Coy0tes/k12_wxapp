page {
  box-sizing: border-box;
  font-size: 36rpx;
}

page>view{
  padding: 20rpx;
}

.box {
  display: flex;
  flex-direction: column;
  border: 1px solid #F7F7F7;
  box-shadow: 4rpx 4rpx 14rpx #EAEAEA;
  border-radius: 10rpx;
  box-sizing: border-box;
  margin-bottom: 40rpx;
}

.box .bottomDetail{
  display: flex;
  flex-direction: column;
  color: #292929;
  padding: 20rpx;
  box-sizing: border-box;
}

.box .bottomDetail>view {
  margin-bottom: 40rpx;
}

.bigBox{
  width: 100%;
  justify-content: space-between;
}

.lrBox {
  display: flex;
  flex-direction: row;
  justify-content: between;
  align-items: center;
  color: #292929;
  position: relative;
  box-sizing: border-box;
  font-size: 32rpx;
}

.textCenter {
  text-align: center;
}

.centerTit{
  width: 100%;
  height: 90rpx;
  background-color: #8DCDF9;
  color: #292929;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx;
}

.other{
  margin-left: 20rpx;
}

.hasTake{
  position: absolute;
  right: 40rpx;
  top: 0;
  font-size: 28rpx;
}

.noTake{
  position: absolute;
  right: 40rpx;
  top: 0;
  font-size: 28rpx;
}

.inline view{
  display: inline;
  line-height: 55rpx;
}

.stop{
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.stop view{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  background-color: #FE8F9F;
  color: #ffffff;
  font-size: 32rpx;
  text-align: center;
}

.around{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.topTitle{
  width: 100%;
  height: 80rpx;
  background-color: #71BFFF;
  color: #ffffff;
  display: flex;
  justify-content: center;
  align-items: center;
}

.stop .submitAgin{
  width: 98%;
  height: 70rpx;
  line-height: 70rpx;
  border: 1px solid #F7F7F7;
  box-shadow: 4rpx 4rpx 10rpx #EBEBEB;
  border-radius: 10rpx;
  background-color: #ffffff;
  color: #292929;
  font-size: 32rpx;
  text-align: center;
}