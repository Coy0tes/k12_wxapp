/**app.wxss**/
@import "/wxParse/wxParse.wxss";
@import "compoment/toast/toast.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.longFont{
  word-wrap: break-word;
  word-break: normal;
}

.bigForm{
  display: block;
  width: 100%;
  height: 100%;
}

@font-face {
    font-family: 'siyuan';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('SourceHanSansCN-Light-Alphabetic.ttf') format('truetype'),
        url('SourceHanSansCN-Light-Alphabetic.svg#SourceHanSansCN-Light-Alphabetic') format('svg');
    font-weight: 300;
    font-style: normal;
}


page{
  font-family: "siyuan";
  overflow: auto;
}